{
  "name": "lgcu_website",
  "version": "1.0.0",
  "description": "LENKESON GLOBAL CHRISTIAN UNIVERSITY WEBSITE",
  "main": "index.js",
  "scripts": {
    "start": "node server.js",
    "build-css": "less-watch-compiler --run-once --main-file=app.less src/css/ public/css/",
    "watch-css": "npm run build-css && less-watch-compiler --main-file=app.less src/css/ public/css/",
    "build": "npm run build-css && react-scripts build",
    "heroku-postbuild": "npm run build",
    "script-run": "react-scripts start",
    "script-dev": "concurrently \"npm run watch-css\" \"npm run script-run\" \"wait-on http://localhost:3001\""
  },
  "author": "Kris Redding",
  "license": "ISC",
  "dependencies": {
    "authorizenet": "^1.0.7",
    "axios": "^0.19.0",
    "bootstrap": "^4.3.1",
    "country-icons": "^1.0.2",
    "dotenv": "^8.0.0",
    "history": "^4.9.0",
    "jquery": "^3.4.1",
    "mongodb": "^3.2.3",
    "nodemailer": "^6.2.1",
    "query-string": "^6.8.1",
    "react": "^16.8.1",
    "react-alice-carousel": "^1.13.1",
    "react-bootstrap": "^1.0.0-beta.11",
    "react-dom": "^16.8.1",
    "react-responsive-carousel": "^3.1.46",
    "react-router-dom": "^4.3.1",
    "react-scripts": "^3.0.1",
    "react-spring": "^8.0.20"
  },
  "devDependencies": {
    "concurrently": "^4.1.0",
    "less": "^3.9.0",
    "less-loader": "^4.1.0",
    "less-watch-compiler": "^1.13.0"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
